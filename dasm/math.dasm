func S {
  star {
    define read c;
    if ne c ' ' {
      if ne c '\t' {
        if ne c '\r' {
          if ne c '\n' {
            fail;
          }
        }
      }
    }
  }
}

func digits {
  plus {
    define read c;
    if lt c '0' {
      fail;
    }
    if gt c '9' {
      fail;
    }
  }
}